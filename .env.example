# Copy this file to .env and fill in your actual values

# Your Chess.com username
CHESS_USER=your_chess_username

# TickTick API token (get from TickTick Developer Console)
TICKTICK_TOKEN=your_ticktick_api_token_here

# Optional: Your email for Chess.com API User-Agent
USER_EMAIL=your_email@example.com

# Optional: Action mode (UPDATE or FINAL)
ACTION_MODE=UPDATE

# Optional: Task title to find in TickTick
TASK_TITLE=Daily chess
